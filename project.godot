; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="Figments"
run/main_scene="res://Direction/Root.tscn"
config/icon="res://icon.png"

[autoload]

Director="*res://Direction/Director.tscn"
Overlay="*res://Direction/Overlay.tscn"
MusicPlayer="*res://Direction/MusicPlayer.tscn"

[display]

window/size/width=1280
window/size/height=720
window/size/resizable=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[locale]

translations=PoolStringArray( "res://Miscellaneous/Translation/text.en.translation", "res://Miscellaneous/Translation/text.it.translation" )

[mono]

project/assembly_name="Figments"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
